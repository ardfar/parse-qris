{
    "name": "ardfar/parse-qris",
    "description": "A Laravel library for parsing QRIS information from QR code images",
    "type": "library",
    "keywords": ["laravel", "qris", "qr-code", "payment", "indonesia"],
    "license": "MIT",
    "authors": [
        {
            "name": "ardfar",
            "email": "ardfar@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "chillerlan/php-qrcode": "^4.3",
        "khanamiryan/qrcode-detector-decoder": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ardfar\\ParseQris\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ardfar\\ParseQris\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ardfar\\ParseQris\\QrisServiceProvider"
            ],
            "aliases": {
                "QrisParser": "Ardfar\\ParseQris\\Facades\\QrisParser"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
